              <body>if (tempTeam == null) {
			try {
				tempTeam = new TeamRoom(mb + &quot;'s team&quot;, srv);
			} catch (Exception e1) {
				e1.printStackTrace();
			}
			
			IInitUser init = connectTo(mb.getIp());
			AInvitation2Chatroom invite = new Invitation2Chatroom((IChatroom) tempTeam, true);

			(new Thread() {
				@Override
				public void run() {
					try {				
						init.receive(me, invite.getDataPacket());
					} catch (Exception e) {
						System.out.println(&quot;Create room failed: &quot; + e + &quot;\n&quot;);
						e.printStackTrace();
					}
				}
			}).start();
			return tempTeam;

		}
		else{
			IInitUser init = connectTo(mb.getIp());
			AInvitation2Chatroom invite = new Invitation2Chatroom((IChatroom) tempTeam, false);
			
			(new Thread() {
				@Override
				public void run() {
					try {				
						init.receive(me, invite.getDataPacket());
					} catch (Exception e) {
						System.out.println(&quot;Create room failed: &quot; + e + &quot;\n&quot;);
						e.printStackTrace();
					}
				}
			}).start();
			
			tempTeam = null;
			return null;
		}		
	</body>
