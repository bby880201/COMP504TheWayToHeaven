						} catch (RemoteException e) {
							System.out.println(&quot;Unknown data type command request failed:&quot;);
							e.printStackTrace();
						}
					}
				}).start();
				
				return &quot;Unknow data type: \&quot;&quot; + newCmdType + &quot;\&quot;, asking for command!&quot;;
			}

